pipeline {
    agent any
    environment {
	DB_HOST = '1000'
	USERNAME = 'ARJUN'
	}

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World, I am making 2nd change'
	 	echo 'My DB host is: ${DB_HOST}'
            }
        }
    
	Stage('Hi') {
	    steps {
		echo 'My name is: ${USERNAME}
	}
    }
	
}
